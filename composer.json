{
  "name": "lessname/hydrator",
  "description": "Hydrate objects",
  "license": "proprietary",
  "repositories": [
    {
      "type": "composer",
      "url": "https://gitlab.com/api/v4/group/6632935/-/packages/composer/packages.json"
    }
  ],
  "require": {
    "php": "^8.2.0",

    "lessname/value-object": "^0.3.2"
  },
  "require-dev": {
    "squizlabs/php_codesniffer": "^3.6.0",
    "phpstan/phpstan": "^1.0.0",
    "phpunit/phpunit": "^10.0.0",
    "vimeo/psalm": "^5.0.0"
  },
  "autoload": {
    "psr-4": {
      "LessHydrator\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "LessHydratorTest\\": "test/"
    }
  },
  "config": {
    "allow-plugins": {
      "composer/package-versions-deprecated": true
    }
  }
}
